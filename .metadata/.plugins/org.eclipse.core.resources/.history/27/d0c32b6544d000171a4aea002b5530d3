package ejercicio113;

import java.util.HashMap;
import java.util.Map;
import java.util.concurrent.ThreadLocalRandom;

public class NIF {
	
	public static void main(String[] args) {		
			
		char letras[]={'T','R','W','A','G','M','Y','F','P','D','X','B','N','J','Z','S','Q',
				'V','H','L','C','K','E'};
		Map<Integer, String> letrasNif = new HashMap<Integer, String>();
		
		for (int i = 0; i < letras.length; i++) {
			letrasNif.put(i, String.valueOf(letras[i]));
			System.out.println(letrasNif.get(i).toString());
		}
		
		for (int i = 0; i < 7; i++) {
			int numerosDNI = ThreadLocalRandom.current().nextInt(1000, 99999999);
		}		
		
	}		
}